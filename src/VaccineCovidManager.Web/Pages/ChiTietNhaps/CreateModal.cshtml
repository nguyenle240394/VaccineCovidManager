@page
@model VaccineCovidManager.Web.Pages.ChiTietNhaps.CreateModalModel
@using Microsoft.Extensions.Localization
@using VaccineCovidManager.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<VaccineCovidManagerResource> L
@{
    Layout = null;
}
<abp-card>
    <form method="post" asp-page="/ChiTietNhaps/CreateModal">
        <abp-modal>
            <abp-modal-header></abp-modal-header>
            <abp-modal-body>
                <abp-select asp-for="NhapVaccine.NoiSxID" asp-items="@Model.NoiSanXuats"></abp-select>
                <abp-select asp-for="NhapVaccine.VaccineId" asp-items="@Model.VaccineCovids"></abp-select>
                <abp-input asp-for="NhapVaccine.NgaySx"/>
                <abp-input asp-for="NhapVaccine.HanSuDung"/>
                <abp-input asp-for="NhapVaccine.SLNhap"/>
            </abp-modal-body>
            <abp-modal-footer buttons="@(AbpModalButtons.Cancel| AbpModalButtons.Save)">
            </abp-modal-footer>
        </abp-modal>
    </form>
</abp-card>